.finished-trip
  img.exit.icon(src='/img/exit.svg', ng-click='exit()')
  h3 Trip Finished!
  h4 How was your trip?

  .stars

  .field
    h6 COMMENTS
    textarea(cols='35', rows='4')

  h4.question How was your caravan?

  .friends
    .friend
      img.photo(src='/img/caleb_small.png')
      .name CALEB H.

    .friend
      img.photo(src='/img/josh_small.png')
      .name JOSH X.

  button(ng-click='done()') DONE
